{
  "name": "newsite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:sass": "sass src/scss/:www/assets/css/ --style compressed --no-source-map ",
    "build:js": "esbuild src/js/index.js --bundle --outfile=www/assets/js/app.js --charset=utf8 --minify",
    "clean:css": "rimraf www/assets/css && mkdir -p www/assets/css",
    "clean:js": "rimraf www/assets/js && mkdir -p www/assets/js",
    "watch:sass": "sass src/scss/:www/assets/css/ --style expanded --watch",
    "watch:js": "esbuild src/js/index.js --bundle --outfile=www/assets/js/app.js --sourcemap --watch --charset=utf8",
    "dev": "npm-run-all -p watch:*",
    "build": "npm run clean:css && npm run build:sass && npm run clean:js && npm run build:js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  }
}
